<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.brbradmin">

    <application
        android:allowBackup="true"
        android:icon="@drawable/people"
        android:label="@string/app_name"
        android:roundIcon="@drawable/people"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".MainActivity"/>
        <activity android:name=".AdminLogin"/>
        <activity android:name=".CustomerList"/>
        <activity android:name=".BarberList"/>
        <activity android:name=".CustomerProfile"/>
        <activity android:name=".BarberProfile"/>
        <activity android:name=".BarberApprovalList"/>
        <activity android:name=".BarberApprovalDetails"/>
    </application>

</manifest>